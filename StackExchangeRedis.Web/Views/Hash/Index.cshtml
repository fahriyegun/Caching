@model Dictionary<string,string>
@{
    ViewData["Title"] = "Index";
}

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="text" name="value" />
    <button type="submit">Kaydet</button>

</form>

@foreach(var item in Model){
    
    <h2>@item.Key -> @item.Value <a asp-controller="Hash" asp-action="Remove" asp-route-item = "@item">Sil</a></h2>
}